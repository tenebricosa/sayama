.table
	width: 100%
	font-size: 12px
	position: absolute
	top: 61px

	&__head
		font-weight: bold

	&__head,
	&__col
		border: 1px solid #fff

	&__head:first-child
	&__col:first-child
		width: 96px

	&__col,
	&__head
		padding: 3px

		&--center
			text-align: center

	&__row

		&--dark
			background-color: #f8cdab
